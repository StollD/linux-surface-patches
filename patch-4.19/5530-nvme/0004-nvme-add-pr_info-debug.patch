From 16ad6117a55036be0dafeca1ffdbd2c2c54e21a0 Mon Sep 17 00:00:00 2001
From: kitakar5525 <34676735+kitakar5525@users.noreply.github.com>
Date: Thu, 11 Jul 2019 08:50:43 +0900
Subject: [PATCH 4/4] nvme: add pr_info debug

---
 drivers/nvme/host/pci.c | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/drivers/nvme/host/pci.c b/drivers/nvme/host/pci.c
index 328247da..db817230 100644
--- a/drivers/nvme/host/pci.c
+++ b/drivers/nvme/host/pci.c
@@ -2610,6 +2610,8 @@ static int nvme_deep_state(struct nvme_dev *dev)
 	struct nvme_ctrl *ctrl = &dev->ctrl;
 	int ret = -EBUSY;;
 
+	pr_info("DEBUG: Passed %s %d \n",__FUNCTION__,__LINE__);
+
 	nvme_start_freeze(ctrl);
 	nvme_wait_freeze(ctrl);
 	nvme_sync_queues(ctrl);
@@ -2653,6 +2655,8 @@ static int nvme_make_operational(struct nvme_dev *dev)
 {
 	struct nvme_ctrl *ctrl = &dev->ctrl;
 
+	pr_info("DEBUG: Passed %s %d \n",__FUNCTION__,__LINE__);
+
 	if (nvme_set_features(ctrl, NVME_FEAT_POWER_MGMT, dev->last_ps,
 			      NULL, 0, NULL) == 0)
 		return 0;
@@ -2665,6 +2669,8 @@ static int nvme_simple_resume(struct device *dev)
 	struct pci_dev *pdev = to_pci_dev(dev);
 	struct nvme_dev *ndev = pci_get_drvdata(pdev);
 
+	pr_info("DEBUG: Passed %s %d \n",__FUNCTION__,__LINE__);
+
 	nvme_reset_ctrl(&ndev->ctrl);
 	return 0;
 }
@@ -2674,6 +2680,8 @@ static int nvme_resume(struct device *dev)
 	struct pci_dev *pdev = to_pci_dev(dev);
 	struct nvme_dev *ndev = pci_get_drvdata(pdev);
 
+	pr_info("DEBUG: Passed %s %d \n",__FUNCTION__,__LINE__);
+
 	return pm_resume_via_firmware() || !ndev->ctrl.npss ?
 		nvme_simple_resume(dev) : nvme_make_operational(ndev);
 }
